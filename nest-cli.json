{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/airlineshub/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/airlineshub/tsconfig.app.json"
  },
  "monorepo": true,
  "projects": {
    "airlineshub": {
      "type": "application",
      "root": "apps/airlineshub",
      "entryFile": "main",
      "sourceRoot": "apps/airlineshub/src",
      "compilerOptions": {
        "tsConfigPath": "apps/airlineshub/tsconfig.app.json"
      }
    },
    "exchange": {
      "type": "application",
      "root": "apps/exchange",
      "entryFile": "main",
      "sourceRoot": "apps/exchange/src",
      "compilerOptions": {
        "tsConfigPath": "apps/exchange/tsconfig.app.json"
      }
    },
    "fidelity": {
      "type": "application",
      "root": "apps/fidelity",
      "entryFile": "main",
      "sourceRoot": "apps/fidelity/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fidelity/tsconfig.app.json"
      }
    },
    "imdtravel": {
      "type": "application",
      "root": "apps/imdtravel",
      "entryFile": "main",
      "sourceRoot": "apps/imdtravel/src",
      "compilerOptions": {
        "tsConfigPath": "apps/imdtravel/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    }
  }
}