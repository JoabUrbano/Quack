{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/airlineshub/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/airlineshub/tsconfig.app.json"
  },
  "monorepo": true,
  "projects": {
    "airlineshub": {
      "type": "application",
      "root": "apps/airlineshub",
      "entryFile": "main",
      "sourceRoot": "apps/airlineshub/src",
      "compilerOptions": {
        "tsConfigPath": "apps/airlineshub/tsconfig.app.json"
      }
    },
    "imdtravel": {
      "type": "application",
      "root": "apps/imdtravel",
      "entryFile": "main",
      "sourceRoot": "apps/imdtravel/src",
      "compilerOptions": {
        "tsConfigPath": "apps/imdtravel/tsconfig.app.json"
      }
    }
  }
}